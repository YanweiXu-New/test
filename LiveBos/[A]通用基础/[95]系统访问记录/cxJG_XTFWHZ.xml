    <query>
        <sql>{"statement":"SELECT {0} AS KSRQ,{1} AS JZRQ,YM, FWCS,\r\nFWCS/CASE WHEN TS &gt; 0 THEN TS ELSE 1 END AS PJCS\r\n  FROM (SELECT YM, SUM(FWCS) AS FWCS,(nvl(TO_DATE({1},'YYYYMMDD'),sysdate)-TO_DATE({0},'YYYYMMDD')) AS TS\r\n          FROM (SELECT SUBSTR(YM, 1, INSTR(YM||'/', '/') - 1) AS YM,FWCS\r\n                  FROM (SELECT SUBSTR(CZSM, 4, INSTR(CZSM||'(', '(') - 4) AS YM, 1 AS FWCS\r\n                  \t      FROM JGCRM.TYHCZRZ A\r\n                 \t     WHERE CZY != 0 AND  ({0} IS NULL OR RQ &gt;= {0}) AND ({1} IS NULL OR RQ &lt;= {1})))\r\n         GROUP BY YM)\r\n ORDER BY FWCS DESC,YM ASC","args":[{"expression":"parameter.PKSRQ","scale":0,"length":10,"type":13},{"expression":"parameter.PJZRQ","scale":0,"length":10,"type":13}],"type":0}</sql>
