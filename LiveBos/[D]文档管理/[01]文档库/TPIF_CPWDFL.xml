            <expression>&lt;%@ livebos language='javascript' %&gt;
var bm;
if(FID==0)
{bm='10';}
else
{
bm=ABS_SQLVALUE("SELECT CASE WHEN T.BM IS NULL THEN (SELECT BM FROM PIF.TPIF_CPWDFL WHERE ID ="+FID+")||'01'  ELSE T.BM END BM FROM (SELECT TO_CHAR(MAX(TO_NUMBER(b.BM))+1) BM FROM PIF.TPIF_CPWDFL A,(SELECT BM FROM PIF.TPIF_CPWDFL  WHERE FID ="+FID+") B WHERE INSTR(b.BM, a.BM) = 1 AND (LENGTH(b.BM) - LENGTH(a.BM)) = 2) T");}
bm; </expression>
            <invisible-expression>&lt;%@ livebos language='javascript' %&gt;
BM=='01'||Type!=2 </invisible-expression>
