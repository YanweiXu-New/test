      <describe>赋值</describe>
      <copy>
        <from type="expression">&lt;%@ livebos language='javascript' %&gt;
 ABS_SQLVALUE("SELECT 'TSJDRMB.MB.'||ID FROM JGCRM.TSJDRMB WHERE MBBM = 'BMSRDR_GS' ");</from>
