    <query>
        <sql>{"statement":"SELECT YH AS 用户,BM AS 部门, CZSJ AS 操作时间,YHIP AS 用户IP\r\n  FROM (SELECT ID,\r\n               (SELECT NAME FROM JGCRM.TUSER WHERE ID = CZY) AS YH,\r\n               (SELECT NAME FROM JGCRM.LBORGANIZATION WHERE ID = YYB) AS BM,\r\n               SUBSTR(CZSM, 4, INSTR(CZSM||'(', '(') - 4) AS YM,\r\n               TO_CHAR(TO_DATE(RQ, 'yyyy-mm-dd'), 'yyyy-mm-dd') || ' ' || FSSJ AS CZSJ,\r\n               CZZD AS YHIP\r\n          FROM JGCRM.TYHCZRZ A\r\n         WHERE CZY != 0  AND ({0} IS NULL OR RQ &gt;= {0}) AND ({1} IS NULL OR RQ &lt;= {1})) T\r\n WHERE T.YM = {2}\r\n ORDER BY ID DESC","args":[{"expression":"parameter.PKSRQ","scale":0,"length":10,"type":13},{"expression":"parameter.PJZRQ","scale":0,"length":10,"type":13},{"expression":"parameter.PYM","scale":0,"length":0,"type":0}],"type":0}</sql>
