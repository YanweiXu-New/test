<?xml version="1.0" encoding="GB2312"?>
<process xmlns="http://www.livebos.com/bizlogic/process" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" attribute="0" exitOnStandardFault="false" isHorizontalLayout="false" name="lcIB_XZHLC_OperateInitialize_UI" object="lcIB_XZHLC" type="1" xsi:schemaLocation="http://www.livebos.com/bizlogic/process http://www.livebos.com/bizlogic_process.xsd">
  <describe>界面初始化界面事件</describe>
  <variables>
    <variable describe="V_FJXX" messageType="" name="V_FJXX" type="resultSet" />
  </variables>
  <if id="0">
    <describe>判断</describe>
    <conditions>
      <condition name="流程未提交" type="expression">
        <arg name="expression">&lt;%@ livebos language='javascript' %&gt;
O_MASTER.InstID==null ||O_MASTER.InstID&lt;0</arg>
      </condition>
    </conditions>
    <sequence id="0" name="顺序">
      <invoke component="SQL" id="0" name="数据库组件_SQL查询" operation="query">
        <describe>数据库组件_SQL查询</describe>
        <arg name="TranType">sql</arg>
        <arg name="SQL">["SELECT ID,wdlxm,tjsx,XM360ZS,SFYY,sfbt FROM tIB_WDLXPZ WHERE lcbd='lcIB_XZHLC' ORDER by tjsx",[]]</arg>
        <arg name="datasource">ECIF</arg>
        <arg name="@retResultSet" type="output">V_FJXX</arg>
      </invoke>
      <invoke component="FormUIHypotOperate" id="0" name="表格操作_添加结果集数据" operation="insertByResult">
        <describe>表格添加结果集数据[询证函流程]</describe>
        <arg name="@objectName">lcIB_XZHLC</arg>
        <arg name="@object" type="variable">O_MASTER</arg>
        <arg name="@columnName">XYFJ</arg>
        <arg name="@clearRowsets">true</arg>
        <arg name="@resultSet" type="variable">V_FJXX</arg>
        <arg flag="field" name="TJSX">TJSX</arg>
        <arg flag="field" name="FJLX">ID</arg>
        <arg flag="field" name="SF360XS">XM360ZS</arg>
        <arg flag="field" name="SFYY">SFYY</arg>
        <arg flag="field" name="SFBT">sfbt</arg>
      </invoke>
    </sequence>
  </if>
</process>

